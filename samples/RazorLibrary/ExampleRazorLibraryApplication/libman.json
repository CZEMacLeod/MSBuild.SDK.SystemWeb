{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "files": [ "jquery.js" ],
      "destination": "lib/jquery/"
    },
    {
      "library": "jquery-validate@1.19.2",
      "files": [ "jquery.validate.js" ],
      "destination": "lib/jquery-validate/"
    },
    {
      "library": "modernizr@2.8.3",
      "files": [ "modernizr.js" ],
      "destination": "lib/modernizr/"
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap@3.4.1",
      "files": ["dist/**/*.*"],
      "destination": "lib/bootstrap/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "files": ["jquery.validate.unobtrusive.js"],
      "destination": "lib/jquery-validation-unobtrusive/"
    }
  ]
}